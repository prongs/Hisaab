<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pack.pack"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk android:minSdkVersion="10" />
    <uses-permission android:name="android.permission.READ_CONTACTS"/>

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" >
        <activity
            android:name=".HisaabActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".OneTripActivity" android:label="@string/loading">
        </activity><activity
            android:name=".TripActivity"
            android:label="@string/new_trip" >
        </activity>
        
        <activity
            android:name=".EventActivity"
            android:label="@string/event" >
        </activity>
        <activity android:name=".TripSummaryActivity"><activity android:name=".TripSummaryActivity"></activity></activity>
        <activity android:name=".TripDistributeActivity"></activity>
    </application>

</manifest>